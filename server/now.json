{
  "version": 2,
  "name": "travel-log",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "src/index.js"
    }
  ],
  "alias": [
    "travel-log"
  ],"env": {
    "NODE_ENV": "production",
    "PORT": "1337",
    "DATABASE_URL": "@travel-log-db",
    "CORS_ORIGIN": "*",
    "secretAccessKey": "@secretaccesskey",
    "accessKeyId" :"@accesskeyid",
    "region" :"@region"
  }
}